{
    "name": "vairogs/vairogs",
    "description": "⛨ Vairogs ⛨: Set of symfony tools and bundles to make coding a bit easier",
    "type": "library",
    "homepage": "https://github.com/vairogs/vairogs/wiki",
    "keywords": [
        "auth",
        "cache",
        "captcha",
        "core",
        "extra",
        "openid",
        "openidconnect",
        "sitemap",
        "steam",
        "utils",
        "vairogs",
        "twig"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dāvis Zālītis (k0d3r1s)",
            "email": "k0d3r1s@vairogs.com",
            "homepage": "https://me.k0d3r1s.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@vairogs.com",
        "issues": "https://github.com/vairogs/vairogs/issues",
        "wiki": "https://github.com/vairogs/vairogs/wiki"
    },
    "funding": [
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/k0d3r1s"
        },
        {
            "type": "paypal",
            "url": "https://www.paypal.com/paypalme/k0d3r1s"
        },
        {
            "type": "issuehunt",
            "url": "https://issuehunt.io/r/vairogs/vairogs"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "doctrine/collections": "*",
        "doctrine/dbal": "*",
        "doctrine/orm": "*",
        "jetbrains/phpstorm-attributes": "*",
        "lcobucci/jwt": "*",
        "league/oauth2-client": "*",
        "psr/cache": "*",
        "psr/http-message": "*",
        "symfony/cache": "~5.4|~6.0",
        "symfony/config": "~5.4|~6.0",
        "symfony/console": "~5.4|~6.0",
        "symfony/dependency-injection": "~5.4|~6.0",
        "symfony/event-dispatcher": "~5.4|~6.0",
        "symfony/form": "~5.4|~6.0",
        "symfony/framework-bundle": "~5.4|~6.0",
        "symfony/http-foundation": "~5.4|~6.0",
        "symfony/http-kernel": "~5.4|~6.0",
        "symfony/options-resolver": "~5.4|~6.0",
        "symfony/property-access": "~5.4|~6.0",
        "symfony/property-info": "~5.4|~6.0",
        "symfony/routing": "~5.4|~6.0",
        "symfony/security-core": "~5.4|~6.0",
        "symfony/translation": "~5.4|~6.0",
        "symfony/validator": "~5.4|~6.0",
        "twig/twig": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.5.0"
    },
    "replace": {
        "vairogs/addon": "self.version",
        "vairogs/addon-auth": "self.version",
        "vairogs/addon-auth-openid": "self.version",
        "vairogs/addon-auth-openid-steam": "self.version",
        "vairogs/auth": "self.version",
        "vairogs/auth-openid": "self.version",
        "vairogs/auth-openidconnect": "self.version",
        "vairogs/cache": "self.version",
        "vairogs/cache-utils": "self.version",
        "vairogs/captcha": "self.version",
        "vairogs/core": "self.version",
        "vairogs/extra": "self.version",
        "vairogs/extra-constants": "self.version",
        "vairogs/extra-specification": "self.version",
        "vairogs/sitemap": "self.version",
        "vairogs/twig": "self.version",
        "vairogs/twig-helper": "self.version",
        "vairogs/twig-helper-date": "self.version",
        "vairogs/twig-helper-email": "self.version",
        "vairogs/twig-helper-file": "self.version",
        "vairogs/twig-helper-gravatar": "self.version",
        "vairogs/twig-helper-http": "self.version",
        "vairogs/twig-helper-identification": "self.version",
        "vairogs/twig-helper-iteration": "self.version",
        "vairogs/twig-helper-json": "self.version",
        "vairogs/twig-helper-php": "self.version",
        "vairogs/twig-helper-sort": "self.version",
        "vairogs/twig-helper-sortlatvian": "self.version",
        "vairogs/twig-helper-text": "self.version",
        "vairogs/twig-helper-uri": "self.version",
        "vairogs/twig-helper-util": "self.version",
        "vairogs/twig-pagination": "self.version",
        "vairogs/twig-php": "self.version",
        "vairogs/utils": "self.version"
    },
    "suggest": {
        "ext-gmp": "to use gmp prime check function",
        "ext-phpiredis": "For PhpRedis cache adapter",
        "ext-redis": "For PhpRedis and Predis cache adapter",
        "doctrine/dbal": "For Orm cache adapter usage",
        "doctrine/orm": "For Orm cache adapter usage",
        "snc/redis-bundle": "For Predis or PhpRedis cache adapter usage",
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\": "src/Vairogs/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vairogs\\Assets\\": "tests/assets",
            "Vairogs\\Tests\\": "tests/src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "thanks": {
            "name": "vairogs/vairogs",
            "url": "https://github.com/vairogs/vairogs"
        }
    }
}
