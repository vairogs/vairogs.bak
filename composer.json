{
    "name": "vairogs/vairogs",
    "description": "⛨ Vairogs ⛨: Set of PHP (mostly Symfony) tools and libraries to make development a bit easier",
    "type": "library",
    "homepage": "https://github.com/vairogs/vairogs/wiki",
    "keywords": [
        "addon",
        "auth",
        "cache",
        "captcha",
        "common",
        "core",
        "extra",
        "position",
        "sitemap",
        "twig",
        "utils",
        "vairogs"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dāvis Zālītis (k0d3r1s)",
            "email": "k0d3r1s@vairogs.com",
            "homepage": "https://me.k0d3r1s.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@vairogs.com",
        "issues": "https://github.com/vairogs/vairogs/issues",
        "wiki": "https://github.com/vairogs/vairogs/wiki"
    },
    "funding": [
        {
            "type": "ko-fi",
            "url": "https://ko-fi.com/k0d3r1s"
        },
        {
            "type": "paypal",
            "url": "https://www.paypal.com/paypalme/k0d3r1s"
        },
        {
            "type": "issuehunt",
            "url": "https://issuehunt.io/r/vairogs/vairogs"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-random": "*",
        "ext-zend-opcache": "*",
        "composer-runtime-api": ">=2.0.0",
        "doctrine/collections": "*",
        "doctrine/dbal": "*",
        "doctrine/orm": "*",
        "jetbrains/phpstorm-attributes": "*",
        "lcobucci/jwt": "*",
        "league/oauth2-client": "*",
        "nikic/php-parser": "*",
        "psr/cache": "*",
        "psr/http-message": "*",
        "simpletoimplement/dependency-injection": "*",
        "symfony/cache": "^6.2|^6.3",
        "symfony/config": "^6.2|^6.3",
        "symfony/console": "^6.2|^6.3",
        "symfony/dependency-injection": "^6.2|^6.3",
        "symfony/event-dispatcher": "^6.2|^6.3",
        "symfony/finder": "^6.2|^6.3",
        "symfony/form": "^6.2|^6.3",
        "symfony/framework-bundle": "^6.2|^6.3",
        "symfony/http-foundation": "^6.2|^6.3",
        "symfony/http-kernel": "^6.2|^6.3",
        "symfony/intl": "^6.2|^6.3",
        "symfony/options-resolver": "^6.2|^6.3",
        "symfony/property-access": "^6.2|^6.3",
        "symfony/property-info": "^6.2|^6.3",
        "symfony/routing": "^6.2|^6.3",
        "symfony/security-bundle": "^6.2|^6.3",
        "symfony/translation": "^6.2|^6.3",
        "symfony/validator": "^6.2|^6.3",
        "twig/twig": "*"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-random": "*",
        "ext-redis": ">=5.3",
        "ext-zend-opcache": "*",
        "doctrine/dbal": "3.5.x-dev",
        "doctrine/doctrine-bundle": "2.7.x-dev",
        "doctrine/orm": "2.14.x-dev",
        "friendsofphp/php-cs-fixer": "dev-master",
        "phpunit/phpunit": "dev-main",
        "predis/predis": "<2.0.0",
        "psr/container": "*",
        "rector/rector": "dev-main",
        "roave/security-advisories": "dev-latest",
        "sebastian/diff": "dev-main as 4.0.4",
        "snc/redis-bundle": "dev-master",
        "symfony/browser-kit": "*",
        "symfony/framework-bundle": "*",
        "symfony/phpunit-bridge": "*",
        "symfony/proxy-manager-bridge": "*",
        "symfony/runtime": "*"
    },
    "replace": {
        "vairogs/addon": "self.version",
        "vairogs/addon-auth": "self.version",
        "vairogs/addon-auth-openid": "self.version",
        "vairogs/addon-auth-openid-steam": "self.version",
        "vairogs/auth": "self.version",
        "vairogs/auth-openid": "self.version",
        "vairogs/auth-openidconnect": "self.version",
        "vairogs/cache": "self.version",
        "vairogs/captcha": "self.version",
        "vairogs/core": "self.version",
        "vairogs/doctrine-utils": "self.version",
        "vairogs/extra": "self.version",
        "vairogs/extra-constants": "self.version",
        "vairogs/extra-encryption": "self.version",
        "vairogs/extra-encryption-cross": "self.version",
        "vairogs/extra-encryption-xxtea": "self.version",
        "vairogs/graphql": "self.version",
        "vairogs/graphql-client": "self.version",
        "vairogs/graphql-server": "self.version",
        "vairogs/sitemap": "self.version",
        "vairogs/twig": "self.version",
        "vairogs/utils": "self.version",
        "vairogs/utils-locator": "self.version"
    },
    "suggest": {
        "ext-gmp": "to use gmp prime check function",
        "ext-phpiredis": "Allows faster serialization and deserialization of the Redis protocol",
        "ext-redis": "If you want to use redis",
        "predis/predis": "If you want to use predis."
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\": "src/Vairogs/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vairogs\\Tests\\Assets\\": "tests/assets",
            "Vairogs\\Tests\\Entity\\": "tests/entity",
            "Vairogs\\Tests\\Source\\": "tests/src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "thanks": {
            "name": "vairogs/vairogs",
            "url": "https://github.com/vairogs/vairogs"
        }
    }
}
