    "require": {
        "php": ">=8.0.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-zlib": "*",
        "doctrine/annotations": "~1.11",
        "doctrine/collections": "~1.6",
        "doctrine/dbal": "~2.8",
        "doctrine/orm": "~2.6|~3",
        "doctrine/persistence": "~2.1",
        "jetbrains/phpstorm-attributes": "*",
        "monolog/monolog": "~2.1",
        "psr/http-message": "^1",
        "symfony/config": "~5.3",
        "symfony/console": "~5.3",
        "symfony/dependency-injection": "~5.3",
        "symfony/form": "~5.3",
        "symfony/framework-bundle": "~5.3",
        "symfony/http-foundation": "~5.3",
        "symfony/http-kernel": "~5.3",
        "symfony/options-resolver": "~5.3",
        "symfony/routing": "~5.3",
        "symfony/security-core": "~5.3",
        "symfony/translation": "~5.3",
        "symfony/validator": "~5.3",
        "twig/twig": "3.*"
    },
    "require-dev": {
        "vimeo/psalm": "*"
    },
    "replace": {
        "vairogs/addon": "self.version",
        "vairogs/addon-auth": "self.version",
        "vairogs/addon-auth-openid": "self.version",
        "vairogs/addon-auth-openid-steam": "self.version",
        "vairogs/auth": "self.version",
        "vairogs/auth-openid": "self.version",
        "vairogs/cache": "self.version",
        "vairogs/cache-utils": "self.version",
        "vairogs/captcha": "self.version",
        "vairogs/components": "self.version",
        "vairogs/extra": "self.version",
        "vairogs/extra-constants": "self.version",
        "vairogs/extra-specification": "self.version",
        "vairogs/sitemap": "self.version",
        "vairogs/twig": "self.version",
        "vairogs/twig-helper": "self.version",
        "vairogs/twig-helper-date": "self.version",
        "vairogs/twig-helper-email": "self.version",
        "vairogs/twig-helper-file": "self.version",
        "vairogs/twig-helper-generator": "self.version",
        "vairogs/twig-helper-gravatar": "self.version",
        "vairogs/twig-helper-http": "self.version",
        "vairogs/twig-helper-identification": "self.version",
        "vairogs/twig-helper-iteration": "self.version",
        "vairogs/twig-helper-json": "self.version",
        "vairogs/twig-helper-php": "self.version",
        "vairogs/twig-helper-sort": "self.version",
        "vairogs/twig-helper-text": "self.version",
        "vairogs/twig-helper-uri": "self.version",
        "vairogs/twig-helper-util": "self.version",
        "vairogs/twig-pagination": "self.version",
        "vairogs/twig-php": "self.version",
        "vairogs/utils": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Vairogs\\": "src/Vairogs/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        },
        "thanks": {
            "name": "vairogs/vairogs",
            "url": "https://github.com/vairogs/vairogs"
